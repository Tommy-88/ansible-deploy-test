
server {
   listen 80;
    server_name {{ domain_name }};

	root opt/guestbook/front;

	index index.html;

  location = /notes {
    proxy_pass http://127.0.0.1:5000;
  }
  location ^~ /comment/ {
    proxy_pass http://127.0.0.1:5000;
  }
  location ^~ /note {
    proxy_pass http://127.0.0.1:5000;
  }

  }

